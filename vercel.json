{
    "version": 2,
    "builds": [
        {
            "src": "./build/index.js",
            "use": "@now/node-server"
        }
    ],
    "routes": [
        {
            "handle": "filesystem"
        },
        {
            "src": "./build/.*",
            "dest": "index.js"
        }
    ]
}